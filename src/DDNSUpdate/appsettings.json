{
  "ConfigurationFiles": {
    "Json": [ "config.json" ]
  },
  "DigitalOcean": {
    "ApiUrl": "https://api.digitalocean.com/v2/"
  },
  "Serilog": {
    "Enrich": [ "FromLogContext" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Using":  [ "Serilog.Sinks.Console" ],
    "WriteTo": [ "Console" ]
  }
}